<?xml version='1.0' encoding='utf-8'?>
<widget id="app.permisos.foedi" version="1.0.0" versionCode="10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Foedi</name>
    <description>
      App Permisos Foedi
  </description>
    <author email="ivan.vazquez@jalisco.gob.mx">
      Ivan Vazquez
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="BackgroundColor" value="0xff0000ff" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="Orientation" value="landscape" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="xwalkMultipleApk" value="false" />
    <preference name="ios-configuration" value="adhoc" />
    <preference name="deployment-target" value="8.0" />
</widget>
